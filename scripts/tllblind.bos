stand()
{
	move pelvis to y-axis [0.000000] speed [0.001111];
	turn pelvis to x-axis <0.000000> speed <200.000000>;
	turn rthigh to x-axis <0.000000> speed <200.000000>;
	turn rleg to x-axis <0.000000> speed <200.000000>;
	turn rfoot to x-axis <0.000000> speed <200.000000>;
	turn lthigh to x-axis <0.000000> speed <200.000000>;
	turn lleg to x-axis <0.000000> speed <200.000000>;
	turn lfoot to x-axis <0.000000> speed <200.000000>;
	sleep 180;
}

MotionControl(Func_Var_1, Func_Var_2)
{
	while( TRUE )
	{
		Func_Var_1 = bMoving;
		Func_Var_2 = bAiming;
		if( Func_Var_1 )
		{
			if( Func_Var_2 )
			{
				Static_Var_3 = 1;
				call-script walklegs2();
			}
			if( !Func_Var_2 )
			{
				Static_Var_3 = 0;
				call-script Walk2();
			}
		}
		if( !Func_Var_1 )
		{
			Static_Var_3 = 1;
			call-script stand();
		}
	}
}

Create()
{
	hide flare1;
	hide flare2;
	hide flare3;
	bMoving = FALSE;
	bAiming = FALSE;
	Static_Var_3 = 1;
	Static_Var_5 = 1;
	gun_1 = torso;
	restore_delay = 3000;
	start-script MotionControl();
	start-script SmokeUnit();
}

StartMoving()
{
	bMoving = TRUE;
}

StopMoving()
{
	bMoving = FALSE;
}

SweetSpot(piecenum)
{
	piecenum = torso;
}

SetMaxReloadTime(Func_Var_1)
{
	restore_delay = Func_Var_1 * 2;
}

Reload()
{
	sleep 4500;
	Static_Var_5 = 1;
}

RestoreAfterDelay()
{
	sleep restore_delay;
	gun_1 = luparm;
	turn torso to y-axis <0.000000> speed <90.000000>;
	turn tbase to y-axis <0.000000> speed <90.000000>;
	turn ruparm to x-axis <0.000000> speed <90.000000>;
	turn ruparm to y-axis <0.000000> speed <100.000000>;
	wait-for-turn torso around y-axis;
	bAiming = FALSE;
}

RestoreAfterDelay2()
{
	sleep restore_delay;
	turn turret to y-axis <0.000000> speed <90.000000>;
	wait-for-turn turret around y-axis;
}

AimFromPrimary(piecenum)
{
	piecenum = torso;
}

QueryPrimary(piecenum)
{
	piecenum = flare1;
}

AimPrimary(heading, pitch)
{
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	while( !Static_Var_5 )
	{
		sleep 25;
	}
	bAiming = TRUE;
	turn torso to y-axis heading speed <100.000000>;
	turn tbase to y-axis <0.000000> - heading speed <100.000000>;
	turn ruparm to x-axis <0.000000> - pitch speed <100.000000>;
	turn ruparm to y-axis <7.000000> speed <100.000000>;
	wait-for-turn torso around y-axis;
	wait-for-turn ruparm around x-axis;
	start-script RestoreAfterDelay();
	return (1);
}

FirePrimary()
{
	move ruparm to z-axis [-1.000000] speed [500.000000];
	if( gun_1 == 0 )
	{
		show flare1;
		sleep 100;
		hide flare1;
	}
	if( gun_1 == 1 )
	{
		show flare2;
		sleep 100;
		hide flare2;
	}
	move ruparm to z-axis [0.000000] speed [2.000000];
	gun_1 = !gun_1;
}

AimSecondary(heading, pitch)
{
	signal SIG_AIM_2;
	set-signal-mask SIG_AIM_2;
	turn turret to y-axis heading speed <100.000000>;
	wait-for-turn turret around y-axis;
	start-script RestoreAfterDelay2();
	return (1);
}

AimFromSecondary(piecenum)
{
	piecenum = turret;
}

QuerySecondary(piecenum)
{
	piecenum = flare3;
}

FireSecondary()
{
}

AimTertiary(heading, pitch)
{
	return (1);
}

FireTertiary()
{
	sleep 150;
}

AimFromTertiary(piecenum)
{
	piecenum = luparm;
}

QueryTertiary(piecenum)
{
	piecenum = hand;
}

